{
  "name": "demo-cli",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript CLI learning demo",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "demo": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "typescript",
    "commander",
    "learning"
  ],
  "author": "Paolo Zhao",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.4.0",
    "open": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/inquirer": "^9.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "prettier": "^3.2.0"
  }
}
